<?xml version="1.0" encoding="utf-8"?>
<Project>
  <ItemGroup Condition="$(NestApprovedFiles) == true">
    <None Update="**\*.approved.*"
          DependentUpon="$(
  [System.String]::Copy('%(FileName)')
    .SubString(
      0,
      $([System.String]::Copy('%(FileName)').IndexOf('.'))
    )
)$(ProjectExt.Replace('proj', ''))" />
  </ItemGroup>
</Project>